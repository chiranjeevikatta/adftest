{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-19df"
		},
		"AzureDatabricksDeltaLake1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake1'"
		},
		"ls_covid19sa4dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covid19sa4dl'"
		},
		"ls_demodb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_demodb'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eforexcel.com/wp/wp-content/uploads/2020/09/Hr1m.zip"
		},
		"LS_AZURE_KEY_VAULT_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://key-vault-formula1.vault.azure.net/"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eforexcel.com/wp/wp-content/uploads/2020/09/Hr1m.zip"
		},
		"ls_covid19sa4dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19sa4dl.dfs.core.windows.net"
		},
		"ls_covid19sa4dl2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19sa4dl.dfs.core.windows.net/"
		},
		"ls_databricks_formula1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0605-184635-hzz9bz2s"
		},
		"BLOB_TRIGGER_properties_PL_BLOB_TRIGGER_parameters_PL_FILE_NAME": {
			"type": "string",
			"defaultValue": "@trigger().outputs.body.fileName"
		},
		"BLOB_TRIGGER_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b117733f-13a4-47e6-858d-481ae1f8d40b/resourceGroups/covid_reporting_rg/providers/Microsoft.Storage/storageAccounts/covid19sa4dl"
		},
		"tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-19df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksDeltaLake1_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake1_accessToken')]"
					},
					"ls_covid19sa4dl_accountKey": {
						"value": "[parameters('ls_covid19sa4dl_accountKey')]"
					},
					"ls_demodb_connectionString": {
						"value": "[parameters('ls_demodb_connectionString')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"LS_AZURE_KEY_VAULT_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AZURE_KEY_VAULT_properties_typeProperties_baseUrl')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"ls_covid19sa4dl_properties_typeProperties_url": {
						"value": "[parameters('ls_covid19sa4dl_properties_typeProperties_url')]"
					},
					"ls_databricks_formula1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_formula1_properties_typeProperties_existingClusterId')]"
					},
					"ls_covid19sa4dl2_properties_typeProperties_url": {
						"value": "[parameters('ls_covid19sa4dl2_properties_typeProperties_url')]"
					},
					"BLOB_TRIGGER_properties_PL_BLOB_TRIGGER_parameters_PL_FILE_NAME": {
						"value": "[parameters('BLOB_TRIGGER_properties_PL_BLOB_TRIGGER_parameters_PL_FILE_NAME')]"
					},
					"BLOB_TRIGGER_properties_typeProperties_scope": {
						"value": "[parameters('BLOB_TRIGGER_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-19df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date": {
						"value": "[parameters('tr_process_formula1_data_properties_pipeline_parameters_p_window_end_date')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-19df_ArmTemplate_0')]"
			]
		}
	]
}